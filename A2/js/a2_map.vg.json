{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Melbourne Cafes & Restaurants â€“ Seats and Types",
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Brian-w-m/FIT3179/main/A2/js/victoriamap.topojson",
        "format": {"type": "topojson", "feature": "Vicsubs.topojson"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {"url": "https://raw.githubusercontent.com/Brian-w-m/FIT3179/main/A2/data/cafes-and-restaurants-with-seating-capacity.csv"},
      "mark": {"type": "circle", "opacity": 0.8, "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "longitude", "type": "quantitative"},
        "latitude": {"field": "latitude", "type": "quantitative"},
        "size": {
          "field": "number_of_seats",
          "type": "quantitative",
          "title": "Number of seats",
          "scale": {"range": [10, 400]}
        },
        "color": {
          "field": "seating_type",
          "type": "nominal",
          "title": "Seating type"
        }
      }
    }
  ]
}


